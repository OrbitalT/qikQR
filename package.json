  "devDependencies": {
    "electron": "8.2.5",
    "electron-builder": "^22.6.0",
    "electron-packager": "^14.2.1",
    "electron-store": "^1.3.0",
    "electron-process-manager": "^0.7.1",
    "electron-reload": "^1.4.0",
    "electron-timber": "^0.5.1"
  },
  "dependencies": {
    "jquery": "^3.5.0",
    "request": "^2.88.0"
  }
