{
  "name": "qikqr",
  "version": "1.0.0",
  "description": "simple app to create QR codes",
  "main": "main.js",
  "author": "dipankar Pal <dipankarpal5050@gmail.com>",
  "contributors": {
    "name": "dipankar pal",
    "email": "dipankarpal5050@gmai.com"
  },
 "files": [
    "*.js",
    "app",
    "build",
    "node_modules"
  ],
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron .",
    "pack": "build --dir",
